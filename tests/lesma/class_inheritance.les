class DemoClass
    x: int
    def new()
        self.x = 5

    def test()
        print("Test")

class DemoClass2: DemoClass
    y: int
    def new()
        self.y = 4
        self.x = 5

    def test2()
        print("Test2")

class DemoClass3: DemoClass2
    z: int
    def new()
        self.z = 6
        self.y = 4
        self.x = 5

    def test3()
        print("Test3")

y = DemoClass3()
y.test()
print(y.x)
print(y.y)
print(y.z)
y.test3()